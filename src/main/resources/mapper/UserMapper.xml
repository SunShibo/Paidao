<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.solland.paidao.dao.UserDAO">

    <select id="getCount" resultType="int" parameterType="String">
        select count(id) from test
    </select>

    <insert id="insert" parameterType="com.solland.paidao.entity.UserDO" useGeneratedKeys="true" keyProperty="id">
        <![CDATA[
           INSERT INTO pd_user_profile( username, password ) VALUES( #{username}, #{password} )
        ]]>
    </insert>

</mapper>